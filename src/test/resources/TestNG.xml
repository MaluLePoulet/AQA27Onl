<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Main Suite" parallel="classes" thread-count="2">
    <test name="Smoke">
        <classes>
            <class name="AUnitTest">
                <methods>
                    <include name="test1"/>
                </methods>
            </class>
            <class name="CalcTest">
                <methods>
                    <exclude name="testDivInt1"/>
                </methods>
            </class>
        </classes>
    </test>
    <test name="Regression">
        <classes>
            <class name="CalcTest">
                <methods>
                    <include name="testDivInt1"/>
                </methods>
            </class>
        </classes>
    </test>
</suite>